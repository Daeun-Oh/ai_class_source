    "import torch\n",
    "\n",
    "is_available = torch.cuda.is_available()\n",
    "device = torch.device(\"cuda\" if is_available else \"cpu\")\n",
    "\n",
    "print(\"CUDA available:\", is_available)\n",
    "print(\"Device:\", device)\n",
    "if is_available:\n",
    "    print(\"GPU name:\", torch.cuda.get_device_name(0))"
   ]
  }
